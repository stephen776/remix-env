{
  "name": "remix-env",
  "version": "0.1.0",
  "description": "Typesafe env for remix with zod",
  "main": "index.js",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephen776/remix-env.git"
  },
  "keywords": [
    "remix",
    "zod",
    "env",
    "environment"
  ],
  "author": "Stephen Jacobs",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stephen776/remix-env/issues"
  },
  "homepage": "https://github.com/stephen776/remix-env#readme",
  "peerDependencies": {
    "typescript": ">=4.7.2",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "zod": "^3.21.4"
  }
}
